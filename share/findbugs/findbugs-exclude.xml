<?xml version="1.0" encoding="UTF-8"?>

<FindBugsFilter>
    <Match>
        <Class name="~.*\.R" />
    </Match>
    <Match>
        <Class name="~.*\.R\$.*" />
    </Match>
    <Match>
        <Class name="~.*Application|.*Service" />
        <Bug pattern="ST_WRITE_TO_STATIC_FROM_INSTANCE_METHOD"></Bug>
    </Match>
    <Match>
        <Bug pattern="DM_EXIT"></Bug>
    </Match>
    <Match>
        <Bug code="MS"></Bug>
    </Match>
</FindBugsFilter>
