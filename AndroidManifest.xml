<?xml version="1.0" encoding="utf-8"?>
<!-- the android:icon was created using http://android-ui-utils.googlecode.com/hg/asset-studio/dist/icons-launcher.html -->
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        android:versionCode="1"
        android:versionName="1.0"
        package="ca.chaves.familyBrowser">
    <uses-sdk android:minSdkVersion="4" /> <!-- Android 1.6 is SDK API 4 -->
    <application
            android:description="@string/app_description"
            android:icon="@drawable/ic_launcher"
            android:label="@string/app_name"
            android:name="android.app.Application">
        <activity
                android:name=".activities.BrowserActivity"
                android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>
